sidebar:
  for_page: [notice, updateList, navigation, last, tagcloud, webinfo]
  for_post: [notice, navigation, toc ]
  widget_library:
    last:
      class: last
      display: [desktop, mobile]
      header:
        icon: fal fa-clock WISTERIA
        title: 最近更新
    toc:
      display: [desktop, mobile]
      header:
        icon: fal fa-list ASBESTOS
    category:
      display: [desktop]
      header:
        icon: fal fa-folder-open ASBESTOS
        url: /categories/
    tagcloud:
      display: [desktop, mobile]
      header:
        title: 标签列表
        icon: fal fa-tags WISTERIA 
        url: /tags/
      max_font: 24
      min_font: 16
    webinfo:
      display: [desktop]
      header:
        icon: fal fa-award NEPHRITIS
      type:
        runtime:
          data: '2018/03/15'
        wordcount:
          unit: ''
        visitcounter:
          siteuv:
            enable: false
          sitepv:
            enable: false
    notice:
      class: text
      display: [desktop]
      header:
        icon: fal fa-ghost PETERRIVE 
        title: 公共信息
      content:
        - '<p>今天你恰饭了吗？  <svg class="iconfont emoji" aria-hidden="true"><use xlink:href="#icon-kaixin"></use></svg></p>'
    updateList:
      class: text
      display: [desktop]
      header:
        icon: fal fa-praying-hands SUNFLOWER 
        title: 更新日志
      content:
        - 1.上线文章页阅读模式(右键开启)
        - 2.音乐播放时启用右键菜单项控制
        - 3.添加了文章页自定义背景显示
    # 自定义 站内导航    
    navigation:
      class: grid
      display: [desktop, mobile]
      header:
        icon: fal fa-map-signs POMEGRANATE 
        title: 站内导航
      fixed: true # 固定宽度
      rows:
        - name: 文档笔记
          icon: fal fa-books GREENSEA
          url:  /page/notes/
        - name: 关于本站
          icon: fal fa-drafting-compass NEPHRITIS
          url: /page/about/  
        - name: 建站历史
          icon: fal fa-leaf-heart POMEGRANATE
          url: /page/timeline/
        - name: 站内留言
          icon: fal fa-comment-alt-smile WISTERIA
          url: /page/info/
        - name: 小伙伴们
          icon: fal fa-stamp CONCRETE
          url: /page/friends/
        - name: 十年之约
          icon: fal fa-glass-cheers PETERRIVE
          url: /page/foreverblog/
    wiki-hexo-theme:
      class: list
      display: [desktop, mobile]
      header:
        icon: fal fa-flower-daffodil ASBESTOS
        title: 笔记本
        url: /page/notes/index.html
      rows:
        - name: '<i class="fal fa-kerning"></i> Github Actions'
          url: /page/notes/actions/index.html
        - name: '<i class="fal fa-kerning"></i> Shell Command'
          url: /page/notes/shell/index.html      
        - name: '<i class="fal fa-kerning"></i> Code Language'
          url: /page/notes/code/index.html




